<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="15" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.gradehub.PrintAdmitCardController"
      stylesheets="@com/gradehub/css/styles.css"
      styleClass="vbox">

    <!-- Label and TextArea to display eligible courses -->
    <Label text="Admit Card:" styleClass="label-normal"/>
    <TextArea fx:id="eligibleCoursesTextArea" editable="false" wrapText="true" maxWidth="250" maxHeight="200" styleClass="text-area"/>
    
    <!-- Back Button -->
    <Button text="Back" onAction="#goBack" styleClass="button"/>
</VBox>
